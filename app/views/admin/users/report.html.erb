<div class="container">
  <table class="table table-striped table-hover">
    <% @report.each do |report| %>
      <tr>
        <td>
          <%= image_tag report.get_image(100,100) %>
          <%= report.content %>
        </td>
      </tr>
    <% end %>
  </table>
  <%= paginate @report %>
</div>