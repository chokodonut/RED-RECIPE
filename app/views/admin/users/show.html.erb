<div class="container">
  <table class="table table-striped table-hover">
    <tr>
      <th>ID</th>
      <th>ニックネーム</th>
      <th>メールアドレス</th>
      <th>登録日時</th>
      <th></th>
    </tr>
    <tr>
      <td>
        <%= @user.id %>
      </td>
      <td>
        <%= @user.name %>
      </td>
      <td>
        <%= @user.email %>
      </td>
      <td>
        <%= @user.created_at %>
      </td>
      <td>
        <%= link_to '削除', admin_user_path(@user.id), method: :delete, "data-confirm" => "本当に削除しますか？" %>
      </td>
    </tr>
  </table>
</div>